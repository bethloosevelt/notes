#include <stdio.h>
#include <stdlib.h>


int isOpener(char symbol) {
	for ( int i=0; i<(sizeof(openers)-1); ++i )
	{
		if ( symbol == openers[i] )
		return 1;
	}
	return 0;
}

int main (void) {

	char* openers = (char*) malloc(sizeof(char)*5);
	openers[0] = "(";
	openers[1] = "{";
	openers[2] = "[";
	openers[3] = "<";
	openers[4] = NULL;
	
	char* closers = (char*) malloc(sizeof(char)*5);
	openers[0] = ")";
	openers[1] = "}";
	openers[2] = "]";
	openers[3] = ">";
	openers[4] = NULL;


	int MAXSIZE = 300;

	// initialize a character array to store the line read
	// in from the console
	// read in a string of up to 300 characters (NULL included)
	// storing this beginning at *line
	char* line = (char*) malloc( sizeof(char)*MAXSIZE );
	fgets( line, MAXSIZE, stdin );

	printf("%s", line);	

	char symbol = " ";
	for ( int i=0; symbol!=NULL; ++i )
	{
		symbol = line[i];
		if ( isOpener(symbol) )
			printf("yesss");
		else
			printf("noooo");
	}
	

	return 0;
}
